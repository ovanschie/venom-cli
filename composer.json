{
    "name": "appstract/venom-cli",
    "description": "Venom CLI",
    "keywords": [
        "appstract",
        "venom"
    ],
    "homepage": "https://github.com/appstract/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5",
        "appstract/php-hostsfile": "^0.1.1",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "kherge/box": "^2.7",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Venom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Venom\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "box": "vendor/bin/box"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "phar-builder": {
            "compression": "GZip",
            "name": "venom.phar",
            "output-dir": "../",
            "entry-point": "./venom",
            "include-dev": false
        }
    }
}
