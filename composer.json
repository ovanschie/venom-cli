{
    "name": "appstract/venom-cli",
    "description": "Venom CLI",
    "keywords": [
        "appstract",
        "venom"
    ],
    "homepage": "https://github.com/appstract/venom-cli",
    "license": "MIT",
    "authors": [
        {
            "name": "Olav van Schie",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "appstract/lush-http": "^0.2.0",
        "appstract/php-hostsfile": "^0.2.0",
        "symfony/console": "^3.2",
        "symfony/filesystem": "^3.2"
    },
    "require-dev": {
        "kherge/box": "^2.7",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Venom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Venom\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "box": "vendor/bin/box"
    },
    "config": {
        "sort-packages": true
    }
}
